	}
	if (heading < <150.0>){
		windDirectionX=642;	windDirectionZ= -767;	return 0;
	}
	if (heading < <160.0>){
		windDirectionX=500;	windDirectionZ= -867;	return 0;
	}
	if (heading < <170.0>){
		windDirectionX=342;	windDirectionZ= -940;	return 0;
	}
	if (heading < <178.0>){
		windDirectionX=173;	windDirectionZ= -985;	return 0;
	}
	if (heading >= <178.0>){
		windDirectionX=0;	windDirectionZ= -1000;	return 0;
	}
}